{
  "name": "sitmenow-hostess",
  "version": "0.0.1",
  "description": "Web application to manage reservations",
  "main": "src/index.js",
  "repository": "https://github.com/sitmenow/hostess",
  "author": "stimenow",
  "license": "MIT",
  "scripts": {
    "start": "./node_modules/.bin/parcel ./index.html",
    "build": "./node_modules/.bin/parcel build ./index.html",
    "pretty": "prettier --write \"src/**/*.js\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,css}": "prettier --write",
    "src/**/*.js": [
      "./node_modules/.bin/eslint",
      "git add"
    ]
  },
  "dependencies": {
    "auth0-js": "^9.9.1",
    "lint-staged": "^8.0.4",
    "moment": "^2.24.0",
    "parcel": "^1.11.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.0-0",
    "react-dom": "^16.5.2",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^2.0.0 || ^3.0.0 || ^4.0.0-0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "cssnano": "^4.1.7",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.3",
    "prettier": "^1.14.3"
  }
}
